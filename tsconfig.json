{
  "compilerOptions": {
    "target": "es5", 
    "module": "commonjs", 
    "outDir": "./dist", 
    "baseUrl": "./src",                             
    "paths": {
      "@types/*":["@types/*"],
      "@modules/*":["modules/*"],
      "@config/*":["config/*"],
      "@shared/*":["shared/*"],
    },                                    
    "esModuleInterop": true,     
    "experimentalDecorators": true,              
    "emitDecoratorMetadata": true,                   
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "resolveJsonModule": true
  }
}